        <Label Content="Key:" Height="28" HorizontalAlignment="Left" Margin="64,44,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="102,44,0,0" Name="textBoxKey" VerticalAlignment="Top" Width="120" Focusable="False" />
        <Label Content="Value: " Height="28" HorizontalAlignment="Left" Margin="51,78,0,0" Name="label3" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="102,80,0,0" Name="textBoxValue" IsEnabled="False" VerticalAlignment="Top" Width="57" Focusable="False" />
        <Label Content="Example of a Statistics property" Height="28" HorizontalAlignment="Left" Margin="10,10,0,0" Name="label4" VerticalAlignment="Top" FontWeight="Bold" />
        <Button Content="End action" Height="23" HorizontalAlignment="Left" Margin="192,118,0,0" Name="buttonEnd" VerticalAlignment="Top" Width="84" IsEnabled="False" />
    </Grid>
</UserControl>
